    "from stack import *\n",
    "\n",
    "\n",
    "s = input('Enter the string: ')\n",
    "\n",
    "\n",
    "def reversed_string(s):\n",
    "    \n",
    "    rs=\"\"\n",
    "    st = Stack()\n",
    "\n",
    "    for i in s:\n",
    "        st.push(i)\n",
    "\n",
    "    while not st.isEmpty():\n",
    "        rs+=st.pop()\n",
    "   \n",
    "    return rs\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "reversed_string(s)\n"
   ]
